@advanced color accent " Accent Color " #fc686f
@advanced color avatarColor " Avatar Effect Color (Only Effect2 and Effect4! (the rainbow glow and the morphing purple))" rgb(252, 104, 111)
@advanced color profileColor " Thumbnail Background Color " rgb(255 175 135)
@advanced color textColor " Text Color " #fff
@advanced dropdown textFont " Font (WIP) " {
    interFont "Inter (Default)" <<<EOT
    body * {
        font-family: "Inter", sans-serif !important;
    }
    EOT;
    gothamFont "Gotham" <<<EOT
    body * {
        font-family: "HCo Gotham SSm", sans-serif !important;
    }
    EOT;
    gillSansFont "Gill Sans" <<<EOT
    body * {
        font-family: "Gill Sans", sans-serif !important;
    }
    EOT;
    courierFont "Courier" <<<EOT
    body * {
        font-family: "Courier", slab-serif !important;
    }
    EOT;
}
@advanced dropdown floatingSidebar " Floating Sidebar " {
floatOn "On" <<<EOT
#navigation {
border-radius: 10px;
        height: 800px;
        padding-top: 50px;
        margin-top: var(--sidebarMargin);
        margin-left: var(--sidebarMargin2);
        animation-name: sidebarSwoop;
        animation-duration: 0.8s;
}
   @keyframes sidebarSwoop {
        from {
            margin-left: -100px;
            opacity: 0;
        }
        to {
            margin-left: var(--sidebarMargin2);
            opacity: 1;
        }
    }
.games-navigate.navigate-left {
margin-left: 0px !important;
}
.games-navigate.navigate-right {
margin-right: 0px;
}
EOT;
floatOff "Off" <<<EOT
EOT;
}
@advanced range sidebarMargin " Floating Sidebar Top Margin (Pixels) " 65
@advanced range sidebarMargin2 " Floating Sidebar Left Margin (Pixels) " 10
@advanced range topbarMargin " Floating Topbar Top Margin (Pixels) " 5
@advanced number topbarWidth " Floating Topbar Width (Pixels) " 1882
@advanced range sideButtonDistance " Sidebar Button Location (Pixels) " 14
@advanced dropdown roundTopbar " Floating Topbar " {
roundingOn "On" <<<EOT
#header {
margin-top: var(--topbarMargin);
margin-left: auto;
margin-right: auto;
width: var(--topbarWidth) !important;
border-radius: 8px !important;
box-shadow: 0 0 8px 0 rgba(0,0,0,.3);
animation-name: topbarSwoop;
animation-duration: 0.8s;
}
 @keyframes topbarSwoop {
        from {
            margin-top: -100px;
            opacity: 0;
        }
        to {
            margin-top: var(--topbarMargin);
            opacity: 1;
        }
    }
EOT;
roundingOff "Off" <<<EOT
EOT;
}
